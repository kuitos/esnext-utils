{
  "extends": "tslint:latest",
  "rules": {
	"indent": [
	  true,
	  "tabs"
	],
	"quotemark": [
	  true,
	  "single"
	],
	"max-line-length": [
	  true,
	  140
	],
	"trailing-comma": false,
	"no-shadowed-variable": false,
	"only-arrow-functions": false,
	"no-console": false,
	"ordered-imports": false,
	"object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
